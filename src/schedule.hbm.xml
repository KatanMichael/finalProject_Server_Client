<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hit.java.models.Schedule" table="SCHEDULE">
        <id name="id" type="int" column="id" >
            <generator class="increment"/>
        </id>

        <property name="activityId">
            <column name="ACTIVITY_ID"/>
        </property>

        <property name="name">
            <column name="NAME"/>
        </property>

        <property name="day">
            <column name="DAY"/>
        </property>

        <property name="start">
            <column name="START"/>
        </property>

        <property name="end">
            <column name="END"/>
        </property>
    </class>
</hibernate-mapping>